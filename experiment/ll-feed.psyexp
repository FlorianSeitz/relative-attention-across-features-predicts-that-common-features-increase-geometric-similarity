        <Param name="Begin JS Experiment" updates="constant" val="nAccuracy = 50; // accuracy shown all x trials&amp;#10;accCrit = 90; // criterion for going to test phase&amp;#10;maxTrials = 1500; // maximal number of training trials" valType="extendedCode"/>
        <Param name="Begin JS Routine" updates="constant" val="// Initializes bools&amp;#10;keyPressed = false;&amp;#10;accResp.keys = [];&amp;#10;accMsg = &quot;&quot;;&amp;#10;&amp;#10;// Computes accuracy of last 2 * nAccuracy trials&amp;#10;x = correctResp.slice((- (2 * nAccuracy)));&amp;#10;acc = Math.round(100 * eval(x.join('+')) / x.length);&amp;#10;&amp;#10;if (((acc &gt; accCrit) &amp;&amp; (trialCounter &gt; (2 * nAccuracy)))) {&amp;#10;    traintrials.finished = true;&amp;#10;} else {&amp;#10;    if ((trialCounter === maxTrials)) {&amp;#10;        traintrials.finished = true;&amp;#10;    } else {&amp;#10;        accMsg = `If you continue like this you will be on average ${acc}% correct; it is possible to be 100% correct.\nPlease stay concentrated and try to improve.\nYour goal is to classify every figure correctly.\n\nPress the spacebar to continue learning.`;&amp;#10;    }&amp;#10;}&amp;#10;&amp;#10;// Checks if accuracy message should be displayed&amp;#10;if ((((trialCounter % nAccuracy) === 0) &amp;&amp; (trialCounter !== nAccuracy) &amp;&amp; (trialCounter !== maxTrials))) {&amp;#10;    accOffset = false;&amp;#10;} else {&amp;#10;    accOffset = true;&amp;#10;}" valType="extendedCode"/>
