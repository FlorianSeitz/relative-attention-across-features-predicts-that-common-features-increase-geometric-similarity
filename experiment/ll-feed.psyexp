        <Param name="Each Frame" updates="constant" val="# Displays feedback if key is pressed&amp;#10;if (not keyPressed) and len(accResp.keys) &gt; 0:&amp;#10;    accMsg = &quot;&quot;&amp;#10;    trialEnd = t + intertrial&amp;#10;    keyPressed = True&amp;#10;&amp;#10;if keyPressed and t &gt; trialEnd:&amp;#10;    accOffset = True" valType="extendedCode"/>
